<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.laura.notizen" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JsonFile"
            android:label="@string/title_activity_json_file" >
        </activity>
        <activity
            android:name=".SafeNotes"
            android:label="@string/title_activity_safe_notes" >
        </activity>
        <activity
            android:name=".NotesOverview"
            android:label="@string/title_activity_notes_overview" >
        </activity>
        <activity
            android:name=".test"
            android:label="@string/title_activity_test" >
        </activity>
    </application>

</manifest>
